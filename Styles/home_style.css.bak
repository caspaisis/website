@font-face {
    font-family: 'salma_promedium';
    src: url('salmapromedium-0wooo-webfont.woff2') format('woff2'),
         url('salmapromedium-0wooo-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'salma_promedium_narrow';
    src: url('salmapromediumnarrow-vg5v6-webfont.woff2') format('woff2'),
         url('salmapromediumnarrow-vg5v6-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;

}

html,body {
	margin: 0;
	padding:0;
	overflow-x:hidden;
	text-decoration: none;
	font-family: salma-promedium;
}

.container{
	margin: 0;
	max-width:100%;
	border: 0;
}

#header {
	text-align: center;
}

#header img{
	vertical-align: center;
	max-height: 70px;
}

.NavBar .header .slideshow-container {
    display: inline;
}

.NavBar {
	background-color: #01ccff;
	margin: 0;
	height: 5%;
	font-size: 24px;
}
.NavBar ul{
	list-style-type: none;
	margin: 0;
	padding: 0;
	background-color: #01ccff;
	display: table;
	width: 100%;
}

.NavBar li {
	z-index: 10;
	text-align: center;
	display: table-cell;
	width: 33%;
}

.NavBar a {
	display: block;
  	color: #ffffff;
	padding: 1.5% 1.5% 1.5%;
	height: 10%;
 	text-align: center;
 	text-decoration: none;
	background: #01ccff;
}

.NavBar li a:hover {
  background-color: #356ddd;
  text-decoration: none;
}